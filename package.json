{
  "name": "interslides",
  "productName": "InterSlides",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext ts src/**",
    "tsc": "tsc",
    "watch": "tsc -w",
    "build": "npm run tsc && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https:/github.com/Andreybest/interslides.git"
  },
  "bugs": {
    "url": "https:/github.com/Andreybest/interslides/issues"
  },
  "homepage": "https:/github.com/Andreybest/interslides",
  "keywords": [],
  "author": {
    "name": "AndreyBest",
    "email": "andrey255@live.com"
  },
  "license": "Apache-2.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "interslides"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "electron": "7.1.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "typescript": "^3.7.3"
  }
}
