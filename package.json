{
  "name": "interslides",
  "productName": "InterSlides",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext ts src/**",
    "tsc": "tsc",
    "watch": "tsc -w",
    "build": "npm run tsc && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https:/github.com/Andreybest/interslides.git"
  },
  "bugs": {
    "url": "https:/github.com/Andreybest/interslides/issues"
  },
  "homepage": "https:/github.com/Andreybest/interslides",
  "keywords": [],
  "author": {
    "name": "AndreyBest",
    "email": "andrey255@live.com"
  },
  "license": "Apache-2.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "interslides"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "jszip": "^3.5.0",
    "material-design-icons": "^3.0.1",
    "mkdirp": "^1.0.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/jszip": "^3.1.7",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^13.13.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "electron": "8.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "typescript": "^3.8.3"
  }
}
